{"scratchpad": {"guidelinesUrlPath": "/computer-programming/guidelines/pjs", "contentKindCode": "p", "newUrlPath": "/computer-programming/new/pjs", "sendToPeers": false, "hideFromHotlist": false, "relativeUrl": "/computer-programming/my-entry-or-target-practice/6411711785598976", "originScratchpadId": 6223097608929280, "projectEval": null, "height": 400, "date": "2015-06-17T14:18:02Z", "originSimilarity": 0.014914938242833839, "id": 6411711785598976, "definitelyNotSpam": false, "description": "", "category": null, "originRevisionId": 5649050225344512, "title": "My Entry (or target practice)", "translatedProjectEval": null, "isPublished": false, "slug": "my-entry-or-target-practice", "isChallenge": false, "width": 400, "descriptionHtml": "", "youtubeId": null, "docsUrlPath": "/computer-programming/docs/pjs", "contentKind": "Scratchpad", "type": "scratchpad", "revision": {"tests": "", "code": "/* \nWhat I love about programming:\nIt is a thinking process to create,debug and improve the program.\n*/\nvar circles = [];\n//The noise amount\nvar change = 0;\n//My mess-up on a function to create a polygon\nvar createRP = function(x,y,radius,sides,strokecolor){\n    pushMatrix();\n    translate(x,y);\n    for(var i = 0;i < 361;i += 360/sides){\n        pushMatrix();\n        rotate(i);\n        stroke(strokecolor);\n        line(-radius/2,radius,radius/2,radius);\n        popMatrix();\n    }\n    popMatrix();\n};\n//Actually creates a regular polygon\nvar createRP2 = function(x,y,apothem,sides,strokeColor,fillColor){\n    var sideLength = tan(360/sides/2)*apothem*2;\n    pushMatrix();\n    translate(x,y);\n    for(var i = 0;i < 361;i+=360/sides){\n        pushMatrix();\n        rotate(i);\n        if(fillColor !== \"none\"){\n            fill(fillColor);\n            noStroke();\n            triangle(-sideLength/2,apothem,sideLength/2,apothem,0,0);\n        }\n        if(strokeColor !== \"none\"){\n            stroke(strokeColor);\n            line(-sideLength/2,apothem,sideLength/2,apothem);\n        }\n        popMatrix();\n    }\n    popMatrix();\n};\n//The number of sides\nvar num = 3;\ndraw = function(){\n    pushMatrix();// Credit to T#1B12P for this awesome way to create multi-sized canvas support\n    scale(width/400,height/400);\n    if(dist(mouseX,mouseY,200,200) <= 100){\n        cursor(CROSS);\n    }else{\n        cursor(ARROW);\n    }\n    //The background\n    background(255, 255, 255);\n    createRP(200,200,100,num,color(400*noise(change), 400*noise(change), 400*noise(change)));\n    createRP(200,200,50,num,color(255, 255*noise(change), 255*noise(change)));\n    createRP(200,200,10,num,color(255*noise(change), 255*noise(change), 255*noise(change)));\n    createRP2(370,30,20,num,color(255, 255*noise(change), 255*noise(change)),color(400*noise(change), 400*noise(change), 400*noise(change)));\n    createRP2(370,370,20,num,color(255, 255*noise(change), 255*noise(change)),color(400*noise(change), 400*noise(change), 400*noise(change)));\n    createRP2(30,30,20,num,color(255, 255*noise(change), 255*noise(change)),color(400*noise(change), 400*noise(change), 400*noise(change)));\n    createRP2(30,370,20,num,color(255, 255*noise(change), 255*noise(change)),color(400*noise(change), 400*noise(change), 400*noise(change)));\n    //Adds more sides\n    if(num < 181){\n        num++;\n    }else{//Goes back to three sides if there is more than 180 sides\n        num = 3;\n    }\n    //Move up the noise\n    change+=0.01;\n    for(var i = 0;i < circles.length;i++){\n        fill(0, 0, 0);\n        noStroke();\n        createRP2(circles[i][0],circles[i][1],10,num,\"none\",color(0, 0, 0));\n    }\n    popMatrix();\n};\nmouseClicked = function(){\n    if(dist(mouseX,mouseY,width/2,height/2) <= 100){\n        circles.push([mouseX,mouseY]);\n    }\n};", "created": "2015-06-17T14:18:02Z", "folds": [], "translatedMp3Url": "", "hasAudio": false, "mp3Url": "", "editorType": "ace_pjs", "playback": "", "youtubeId": null, "configVersion": 4, "id": 5