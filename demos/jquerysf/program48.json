  return img;\n};\n\nvar HorizontalGRD = function(clr1, clr2){\n    noFill();\n    strokeWeight(7.5);\n    colorMode(3);\n    var q = 0.03;\n    for (var i = 0; i < width+1; i+=5){\n        for (var j = height/2; j<height+1; j+=5){\n            stroke(lerpColor(clr1, clr2, (j-height/2)/height));\n            point(i, j);\n            point(i, height-j);\n        }\n        clr1 = color(lerpColor(clr1, clr2, i/width*q));\n    }\n    var img = get(0, 0, width, height);\n    return img;\n};\n\nvar VerticalGRD = function(clr1, clr2){\n    noFill();\n    strokeWeight(7.5);\n    colorMode(3);\n    var q = 0.03;\n    for (var i = width/2; i < width+1; i+=5){\n        for (var j = 0; j<height+1; j+=5){\n            stroke(lerpColor(clr1, clr2, j/height));\n            point(i, j);\n            point(width-i, j);\n        }\n        clr1 = color(lerpColor(clr1, clr2, (i-width/2)/width*q));\n    }\n    var img = get(0, 0, width, height);\n    return img;\n};\n\ncolorMode(3);\nvar backgrounds = [\n    {screen:0,clr1:color(254, 255, 255),clr2:color(0, 255, 255),type:\"\\\\\"}, \n    {screen:1,clr1:color(254, 255, 255),clr2:color(0, 255, 255),type:\"-\"}, \n    {screen:2,clr1:color(154, 255, 255),clr2:color(0, 255, 255),type:\"l\"}, ];\n\nvar saveScene = [];\n\n\nvar a = 0;\nvar b = 0;\nvar draw = function() {\n    if (a < backgrounds.length){\n        var img;\n        switch(backgrounds[a].type.toLowerCase()){\n            case \"\\\\\":\n                img = DiagnalGRD(backgrounds[a].clr1, backgrounds[a].clr2);\n                break;\n            case \"-\":\n                img = HorizontalGRD(backgrounds[a].clr1, backgrounds[a].clr2);\n                break;\n            case \"|\":\n            case \"l\":\n                img = VerticalGRD(backgrounds[a].clr1, backgrounds[a].clr2);\n                break;\n        }\n        saveScene[backgrounds[a].screen] = img;\n        a++;\n        image(saveScene[screen], 0, 0);\n    }else if (b < backgrounds.length){\n        image(saveScene[screen], 0, 0);\n        b++;\n    }else{\n        image(saveScene[screen], 0, 0);\n        if (frameCount%200===0){\n            mousePressed();\n        }\n    }\n};\n\nvar mousePressed = function(){\n    if (screen === backgrounds.length-1){\n        screen = 0;\n    }else{\n        screen++;\n    }\n};\n", "created": "2015-06-17T17:34:33Z", "folds": [[15, 24], [26, 41], [43, 58], [60, 75], [117, 123]], "translatedMp3Url": "", "hasAudio": false, "mp3Url": "", "editorType": "ace_pjs", "playback": "", "youtubeId": null, "configVersion": 4, "id": 5668600916475904}, "tests": "", "imagePath": "/computer-programming/gradient-generator/4940885471133696/5668600916475904.png", "nodeType": "scratchpad", "editSlug": "edit/p/gradient-generator", "isProject": false, "tags": [], "translatedDescription": "", "byChild": false, "difficulty": -1, "originIsProject": false, "key": "ag5zfmtoYW4tYWNhZGVteXIXCxIKU2NyYXRjaHBhZBiAgIjb2LbjCAw", "date": "2015-06-17T17:34:33Z", "slug": "gradient-generator", "spinoffCount": 0, "kind": "Scratchpad", "termMap": {"new": "New Program", "guidelines": "Program Guidelines", "restart": "Restart"}, "globalId": "p4940885471133696", "translatedDescriptionHtml": "", "created": "2015-06-16T20:41:40Z", "url": "https://www.khanacademy.org/computer-programming/gradient-generator/4940885471133696", "imageUrl": "https://www.khanacademy.org/computer-programming/gradient-generator/4940885471133696/5668600916475904.png", "translatedTests": "", "sumVotesIncremented": 2, "defaultUrlPath": null, "flags": [], "isProjectOrFork": false, "userAuthoredContentType": "pjs", "kaid": "kaid_1001992926089779129538917", "translatedTitle": "Gradient Generator"}, "embedded": false, "creatorProfile": {"isSelf": false, "username": "Unicoranium", "isParentOfLoggedInUser": false, "isChildAccount": false, "hasChangedAvatar": true, "soundOn": false, "followRequiresApproval": true, "canModifyCoaches": true, "allowedByParentToAddAnyCoach": true, "hasStudents": false, "publi