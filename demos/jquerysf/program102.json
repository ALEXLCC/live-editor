        }\n            if(a<50){\n                a=a+0.1;\n            }\n            if(a>50){\n                a=a+0.3;\n            }\n            if(a>100){\n                a=a+0.2;\n            }\n        }\n        if(b>50&&b<100&&a<200){\n           if(a<50){\n                c=c-2;\n            }\n            if(a>50){\n                c=c-3;\n            }\n            if(a>100){\n                c=c-1;\n            }\n            if(a<50){\n            a=a+0.1;\n            }\n            if(a>50){\n                a=a+0.3;\n            }\n            if(a>100){\n                a=a+0.2;\n            }\n        }\n        fill(0, 0, 0);\n        ellipse(c-a/10,d,a/10,a/10);\n        ellipse(c+a/10,d,a/10,a/10);\n        \n        if(a>199){\n            noFill();\n            stroke(0, 0, 0, e);\n            arc(c,d,100,50,0,180);\n            e++;\n        }\n    }\n};\n", "created": "2015-06-17T16:39:02Z", "folds": [], "translatedMp3Url": "", "hasAudio": false, "mp3Url": "", "editorType": "ace_pjs", "playback": "", "youtubeId": null, "configVersion": 4, "id": 5676830073815040}, "tests": "", "imagePath": "/computer-programming/sammy-the-slithering-snake/5716146361860096/5676830073815040.png", "nodeType": "scratchpad", "description": "", "isProject": false, "tags": [], "translatedDescription": "", "byChild": false, "difficulty": -1, "originIsProject": false, "key": "ag5zfmtoYW4tYWNhZGVteXIXCxIKU2NyYXRjaHBhZBiAgIjL4NmTCgw", "date": "2015-06-17T16:39:02Z", "height": 400, "translatedTests": "", "kind": "Scratchpad", "termMap": {"new": "New Program", "guidelines": "Program Guidelines", "restart": "Restart"}, "globalId": "p5716146361860096", "translatedDescriptionHtml": "", "created": "2015-06-13T11:36:23Z", "url": "https://www.khanacademy.org/computer-programming/sammy-the-slithering-snake/5716146361860096", "imageUrl": "https://www.khanacademy.org/computer-programming/sammy-the-slithering-snake/5716146361860096/5676830073815040.png", "isPublished": false, "